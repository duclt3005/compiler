Program Example2; (* Factorial *)

CONst  d = 123.4 ;
Type t = FLOAT;
Var n : Integer;
    m: Float;
    c : char;
    a:Integer;
     b:Integer;
PROCEDURE  HANOI;
Begin
end;
Function F( n : Integer  ) : Integer;
  Begin
    If n = 0 Then F := 1 Else F := N * F (N - 1);
  end;

Begin
  For n := 1 To 7 Do
    Begin
      a += F(n);
      a -= F(n);
      a *= F(n);
      a /= F(n);
      Call WriteLN;
      CAll HANOI;
      Call WriteI(a);
    End;
End. (* Factorial *)