Program Example2; (* Factorial *)

CONst  d = 123.4 ;
Type t = FLOAT;
Var n : FLOAT;

Function F( n : Integer ; var x : FLOAT ) : Integer;
  Begin
    If n%2 = 0 Then F := 1 Else F := N * F (N - 1);
  end;

Begin
  For n := 1 To 7 Do 
    Begin
      Call WriteLn; 
      Call WriteF( F(n));
      Call F(2);
    End;
End. (* Factorial *)